# Not unix native compressed archive file types and programs handling these

ZIP Archive {
	icon package
	match_name {
		*.zip
		*.ZIP
	}
	&List: %terminal "unzip -l %n | less"
	&Extract: %terminal "unzip %n | less"
}

RAR Archive {
	icon package
	match_name {
		*.rar
		*.RAR
	}
	&List: %terminal "unrar l %n | less"
	&Extract:  %terminal "unrar %n | less"
}

7ZIP Archive {
	icon package
	match_name {
		*.7z
		*.7Z
	}
	&Extract: %terminal "p7zip -d %n | less"
}
