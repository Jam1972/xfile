# Graphics file formats and applications that handle these

GIMP Image {
	icon xcf
	match_name {
		*.xcf
		*.psd
		*.PSD
	}
	action &Open: gimp %n
}

Krita Image {
	icon kra
	match_name *.kra
	action &Open: krita %n
}

Blender Scene {
	icon blend
	match_name *.blend
	action &Open: blender %n
}

Vector Graphics {
	icon ofdraw
	match_name {
		*.svg
		*.svgz
		*.eps
		*.EPS
		*.ai
		*.AI
		*.cdr
		*.CDR
	}
	action &Open: inkscape %n
}

XFig Drawing {
	icon ofdraw
	match_name {
		*.fig
		*.fig.gz
		*.fig.[zZ]
	}
	action &Open: xfig %n
}
